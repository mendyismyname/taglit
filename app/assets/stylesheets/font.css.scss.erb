@font-face {
  font-family: 'bucket-ico-font';
  src:url('bucket-ico-font.eot');
  src:url('bucket-ico-font.eot?#iefix') format('embedded-opentype'),
    font-url('bucket-ico-font.woff') format('woff'),
    font-url('bucket-ico-font.ttf') format('truetype'),
    font-url('bucket-ico-font.svg#bucket-ico-font') format('svg');
  font-weight: normal;
  font-style: normal;
}

